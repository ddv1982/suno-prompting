{
  "name": "groq-suno-prompter",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build:css && electrobun build",
    "build:all": "bun build:css && electrobun build --targets=all",
    "build:macos": "bun build:css && electrobun build --targets=macos-arm64,macos-x64",
    "build:linux": "bun build:css && electrobun build --targets=linux-x64",
    "build:windows": "bun build:css && electrobun build --targets=windows-x64",
    "build:canary": "bun build:css && electrobun build --env=canary",
    "build:canary:all": "bun build:css && electrobun build --env=canary --targets=all",
    "build:canary:macos": "bun build:css && electrobun build --env=canary --targets=macos-arm64,macos-x64",
    "build:canary:linux": "bun build:css && electrobun build --env=canary --targets=linux-x64",
    "build:canary:windows": "bun build:css && electrobun build --env=canary --targets=windows-x64",
    "build:stable": "bun build:css && electrobun build --env=stable",
    "build:stable:all": "bun build:css && electrobun build --env=stable --targets=all",
    "build:stable:macos": "bun build:css && electrobun build --env=stable --targets=macos-arm64,macos-x64",
    "build:stable:linux": "bun build:css && electrobun build --env=stable --targets=linux-x64",
    "build:stable:windows": "bun build:css && electrobun build --env=stable --targets=windows-x64",
    "build:css": "tailwindcss -i src/main-ui/index.css -o src/main-ui/dist.css",
    "start": "bun build:css && electrobun dev",
    "readme:sync": "bun run scripts/generate-readme-genre-table.ts",
    "readme:check": "bun run scripts/generate-readme-genre-table.ts --check",
    "test": "bun test",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "validate": "bun run typecheck && bun run lint && bun run test",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/dompurify": "^3.2.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "husky": "^9.1.7",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.2",
    "@ai-sdk/groq": "^3.0.2",
    "@ai-sdk/openai": "^3.0.2",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/cli": "^4.1.18",
    "ai": "^6.0.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dompurify": "^3.3.1",
    "electrobun": "0.8.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.5"
  },
  "private": true,
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}
