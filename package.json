{
  "name": "groq-suno-prompter",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build:css && electrobun build",
    "build:all": "bun build:css && electrobun build --targets=all",
    "build:macos": "bun build:css && electrobun build --targets=macos-arm64,macos-x64",
    "build:linux": "bun build:css && electrobun build --targets=linux-x64",
    "build:windows": "bun build:css && electrobun build --targets=windows-x64",
    "build:css": "tailwindcss -i src/main-ui/index.css -o src/main-ui/dist.css",
    "start": "bun build:css && electrobun dev"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/dompurify": "^3.2.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@ai-sdk/groq": "^2.0.33",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/cli": "^4.1.18",
    "ai": "^5.0.116",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "electrobun": "^0.7.1",
    "lucide-react": "^0.562.0",
    "react": "^19.0.0-rc.1",
    "react-dom": "^19.0.0-rc.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7"
  },
  "private": true,
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}
